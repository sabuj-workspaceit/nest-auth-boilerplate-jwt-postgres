        {
            "key": "baseUrl",
            "value": "http://localhost:4000/api",
            "type": "string"
        },
        {
            "key": "accessToken",
            "value": "",
            "type": "string"
        },
        {
            "key": "refreshToken",
            "value": "",
            "type": "string"
        },
        {
            "key": "userId",
            "value": "",
            "type": "string"
        },
        {
            "key": "verificationOtp",
            "value": "",
            "type": "string"
        },
        {
            "key": "verificationToken",
            "value": "",
            "type": "string"
